# Claude Evaluator Environment Variables
# Copy this file to .env and fill in your values

# =============================================================================
# REQUIRED: API Keys
# =============================================================================

# Gemini API Configuration (required for evaluator agent)
GEMINI_API_KEY=your_gemini_api_key_here

# =============================================================================
# Worker Agent Configuration
# =============================================================================

# Model identifier for SDK execution
# CLAUDE_WORKER_MODEL=claude-haiku-4-5@20251001

# Maximum conversation turns per query
# CLAUDE_WORKER_MAX_TURNS=10

# Timeout in seconds for question callbacks
# CLAUDE_WORKER_QUESTION_TIMEOUT_SECONDS=60

# =============================================================================
# Developer Agent Configuration
# =============================================================================

# Model identifier for Q&A interactions
# CLAUDE_DEVELOPER_QA_MODEL=claude-haiku-4-5@20251001

# Number of recent messages to include as context
# CLAUDE_DEVELOPER_CONTEXT_WINDOW_SIZE=10

# Maximum retries for rejected answers
# CLAUDE_DEVELOPER_MAX_ANSWER_RETRIES=1

# Maximum loop iterations before forced termination
# CLAUDE_DEVELOPER_MAX_ITERATIONS=100

# =============================================================================
# Evaluator Agent Configuration
# =============================================================================

# Gemini model identifier for evaluation scoring
# CLAUDE_EVALUATOR_MODEL=gemini-2.0-flash

# Timeout for evaluation operations in seconds
# CLAUDE_EVALUATOR_TIMEOUT_SECONDS=120

# LLM temperature for scoring (lower = more deterministic)
# CLAUDE_EVALUATOR_TEMPERATURE=0.1

# Whether to use tree-sitter AST parsing
# CLAUDE_EVALUATOR_ENABLE_AST_PARSING=true

# =============================================================================
# Scoring Weights (must sum to 1.0 for normalized scores)
# =============================================================================

# Weight for task completion score (0.0 to 1.0)
# CLAUDE_EVALUATOR_TASK_COMPLETION_WEIGHT=0.5

# Weight for code quality score (0.0 to 1.0)
# CLAUDE_EVALUATOR_CODE_QUALITY_WEIGHT=0.3

# Weight for efficiency score (0.0 to 1.0)
# CLAUDE_EVALUATOR_EFFICIENCY_WEIGHT=0.2
